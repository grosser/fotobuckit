<h1><%= @job.title %></h1>

<% if current_user %>
  <%= form_for @job, :html => {:class => "full_form"}  do |f| %>
    <%= render 'form', :f => f %>
    <%= submit_tag 'Save' %>
  <% end %>

  <%= render 'access_generator' %>
<% end %>

<% @job.s3_files.each do |file| %>
  <%= image_for file, '200x200' %>
<% end %>
